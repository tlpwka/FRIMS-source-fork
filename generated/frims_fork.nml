# 41 "src/wagon_attach.pnml"
switch(FEAT_TRAINS, SELF, switch_can_attach_vehicle, vehicle_type_id) {
 0x000..0x0FF: switch_can_attach_JP_HS;
 0x100..0x1FF: switch_can_attach_SK_HS;
 0x400..0x4FF: switch_can_attach_ZH_HS;
